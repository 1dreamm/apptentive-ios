platform :ios, '8.0'
use_frameworks!

target 'iOSExample' do
	# This will use the latest version of the Apptentive SDK from CocoaPods
	pod 'apptentive-ios'
	# Uncomment the following line (and comment out the previous one) to use the version of the Apptentive SDK in this repository
	# pod 'apptentive-ios', :path => '..'
end
